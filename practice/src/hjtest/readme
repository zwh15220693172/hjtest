hj112分苹果
hj23数据分类
String hexString = Integer.toHexString(num);
if (hexString.length() % 2 != 0) {
  hexString = "0" + hexString;
}
int sum = 0;
for (int i = 0; i < hexString().length() - 1, i+=2) {
   sum += Integer.parseInt(hexString.substring(i,i+2),16);
}
}

